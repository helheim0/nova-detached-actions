{
    "name": "gobrightspot/nova-detached-actions-dev",
    "description": "A Laravel Nova tool to allow for placing actions in the Nova toolbar detached from the checkbox selection mechanism.",
    "keywords": [
        "laravel",
        "nova",
        "detached actions"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Brightspot\\Nova\\Tools\\DetachedActions\\": "src/"
        }
    },
    "require": {
        "laravel/nova": "^5.0",
        "gobrightspot/nova-detached-actions": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brightspot\\Nova\\Tools\\DetachedActions\\ToolServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/helheim0/nova-detached-actions"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
